<body>
    <canvas id="chart" width="100%" height="450"></canvas>
    <div class="container">
        <input type="text" name="symbol" id="symbol" placeholder="Enter Symbol" onkeyup="this.value = this.value.toUpperCase();">
        <button id="addSymbol">Add</button>
    </div>
    <ul id="ulist"></ul>

    <script>
        let symbolArray = "<%= symbolString %>".split(',');
        symbolArray = symbolArray.slice(0, symbolArray.length - 1);
    </script>
    <script src="script.js"></script>
